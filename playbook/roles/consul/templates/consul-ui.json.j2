{
  "service": {
    "name": "consul-ui",
    "address": "{{ hostvars[ansible_hostname]['ansible_' + network.interface].ipv4.address }}",
    "port": 8500,
    "enableTagOverride": false,
    "checks": [
      {
        "http": "http://{{ hostvars[ansible_hostname]['ansible_' + network.interface].ipv4.address }}:8500/ui",
        "interval": "10s"
      }
    ]
  }
}
